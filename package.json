{
  "name": "minimal-result",
  "version": "1.0.1",
  "description": "A minimal, Flow-typed, Rust-style Result library for functional exception handling.",
  "main": "lib/index.js",
  "scripts": {
    "test": "",
    "build:clean": "rimraf lib",
    "build:lib": "babel -d lib/ src/",
    "build:flow": "flow-copy-source -v src lib",
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
    "prepublish": "npm run build"
  },
  "keywords": [
    "result",
    "fp",
    "exception",
    "flow"
  ],
  "author": "Lanetix",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "flow-copy-source": "^1.2.0",
    "rimraf": "^2.6.1",
    "babel-preset-env": "^1.6.0"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lanetix/minimal-result.git"
  },
  "bugs": {
    "url": "https://github.com/bwestergard/minimal-result/issues"
  },
  "homepage": "https://github.com/bwestergard/minimal-result#readme"
}
